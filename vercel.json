{
    "rewrites": [
        {
            "source": "/assets/(.*)",
            "destination": "/assets/$1"
        },
        {
            "source": "/(.*)\\.([a-z0-9]{8})\\.js",
            "destination": "/$1.$2.js"
        },
        {
            "source": "/(.*)\\.([a-z0-9]{8})\\.css",
            "destination": "/$1.$2.css"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "cleanUrls": true,
    "trailingSlash": false
}